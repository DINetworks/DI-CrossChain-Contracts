{
  "title": "DI-Contracts Documentation",
  "description": "Comprehensive documentation for DI-Contracts omnichain bridging protocol",
  "author": "DI Network Team",
  "language": "en",
  "gitbook": ">=3.0.0",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "code",
    "copy-code-button",
    "expandable-chapters",
    "anchors",
    "search-pro",
    "prism",
    "-highlight"
  ],
  "pluginsConfig": {
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    },
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord": ["Gitbook", "DI-Contracts"]
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "fontFamily": "Arial",
    "paperSize": "a4",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  }
}